{
  "projectType": "library",
  "sourceRoot": "libs/features/history/src",
  "prefix": "orange-finance",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/features/history"],
      "options": {
        "project": "libs/features/history/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/features/history/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/features/history/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/features/history"],
      "options": {
        "jestConfig": "libs/features/history/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/features/history/**/*.ts",
          "libs/features/history/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/features/history/.storybook",
        "browserTarget": "features-history:build-storybook",
        "compodoc": false,
        "styles": ["libs/ui-kit/theme/theme.scss"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/features-history",
        "configDir": "libs/features/history/.storybook",
        "browserTarget": "features-history:build-storybook",
        "compodoc": false,
        "styles": ["libs/ui-kit/theme/theme.scss"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": ["feature"]
}
