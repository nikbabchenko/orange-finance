{
  "projectType": "library",
  "sourceRoot": "libs/ui-kit/components/toolbar/src",
  "prefix": "oe",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/ui-kit/components/toolbar"],
      "options": {
        "project": "libs/ui-kit/components/toolbar/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ui-kit/components/toolbar/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ui-kit/components/toolbar/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ui-kit/components/toolbar"],
      "options": {
        "jestConfig": "libs/ui-kit/components/toolbar/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ui-kit/components/toolbar/**/*.ts",
          "libs/ui-kit/components/toolbar/**/*.html"
        ]
      }
    },
    "storybook": {
      "executor": "@storybook/angular:start-storybook",
      "options": {
        "port": 4400,
        "configDir": "libs/ui-kit/components/toolbar/.storybook",
        "browserTarget": "ui-kit-components-toolbar:build-storybook",
        "compodoc": false,
        "styles": ["libs/ui-kit/theme/theme.scss"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    },
    "build-storybook": {
      "executor": "@storybook/angular:build-storybook",
      "outputs": ["{options.outputDir}"],
      "options": {
        "outputDir": "dist/storybook/ui-kit-components-toolbar",
        "configDir": "libs/ui-kit/components/toolbar/.storybook",
        "browserTarget": "ui-kit-components-toolbar:build-storybook",
        "compodoc": false,
        "styles": ["libs/ui-kit/theme/theme.scss"]
      },
      "configurations": {
        "ci": {
          "quiet": true
        }
      }
    }
  },
  "tags": ["ui-kit"]
}
